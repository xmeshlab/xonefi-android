{
  "name": "xonefiapp",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client -c --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest",
    "web": "expo start --web",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/blur": "^4.3.0",
    "@react-native-community/slider": "^4.4.0",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "^6.4.1",
    "@react-navigation/elements": "^1.3.7",
    "@react-navigation/native": "^6.0.14",
    "@react-navigation/stack": "^6.3.9",
    "@toruslabs/react-native-web-browser": "^1.1.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@web3auth/react-native-sdk": "^3.5.0",
    "chai": "^4.3.7",
    "expo": "~47.0.12",
    "expo-community-flipper": "^47.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "form-data": "^4.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "mocha": "^10.2.0",
    "nativewind": "^2.0.11",
    "node-libs-browser": "^2.2.1",
    "react": "18.1.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-native": "0.70.5",
    "react-native-background-timer": "2.4.1",
    "react-native-charts-wrapper": "^0.5.11",
    "react-native-circular-progress": "^1.3.7",
    "react-native-device-info": "^10.3.0",
    "react-native-flipper": "^0.178.1",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-modal": "^13.0.1",
    "react-native-quick-base64": "^2.0.5",
    "react-native-quick-crypto": "^0.5.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-root-siblings": "^4.1.1",
    "react-native-root-toast": "^3.3.1",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^13.4.0",
    "react-native-sync-storage": "^1.0.0",
    "react-native-udp": "4.1.5",
    "react-native-vector-icons": "^9.2.0",
    "react-native-wifi-reborn": "^4.7.0",
    "react-router-dom": "^6.11.2",
    "tailwind-rn": "^4.2.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "web3": "^1.8.1",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.1",
    "concurrently": "^7.6.0",
    "jest-fetch-mock": "^3.0.3",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/Components/*.{js, jsx}",
      "src/Components/*.ts",
      "src/Components/*.tsx",
      "src/screens/*.{js, jsx}",
      "src/screens/*.ts",
      "src/screens/*.tsx",
      "!src/Components/*.d.ts",
      "!src/screens/*.d.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "jsx": "react"
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "tsx",
      "ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-native-quick-crypto|web3)"
    ]
  },
  "private": true
}
